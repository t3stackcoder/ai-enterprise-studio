{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "type-check": {
      "outputs": []
    },
    "@ai-enterprise-studio/web-shell#dev": {
      "dependsOn": [],
      "cache": false,
      "persistent": true
    },
    "@ai-enterprise-studio/web-chess-analysis#dev": {
      "cache": false,
      "persistent": true
    },
    "@ai-enterprise-studio/web-video-analysis#dev": {
      "cache": false,
      "persistent": true
    },
    "@ai-enterprise-studio/analysis-server#dev": {
      "cache": false,
      "persistent": true
    },
    "@ai-enterprise-studio/video-analysis-server#dev": {
      "cache": false,
      "persistent": true
    },
    "@ai-enterprise-studio/web-shell#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@ai-enterprise-studio/web-chess-analysis#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@ai-enterprise-studio/web-video-analysis#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}


